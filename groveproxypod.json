{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "groveproxy",
    "namespace": "dev",
    "labels": {
      "service": "groveproxy",
      "group": "dev"
    }
  },
  "spec": {
    "serviceAccountName": "groveproxy",
    "containers": [{
      "name": "proxy",
      "image": "docker.elear.solutions/cocogrovehttpproxy:latest",
      "env": [{ "name": "PORT", "value": "8080" },
              { "name": "MONGO_DB_URL","value":"mongodb://mongodb.cocostore.dev.svc.cluster.local:27017" },
              { "name": "OAUTH_SERVICE","value":"http://oauth-server.cocostore.dev.svc.cluster.local:8080" },
              { "name": "DB_NAME","value":"groveapps" },
              { "name": "AWS_S3_ACCESS_KEY_ID","value":"AKIAIQLAALYKYMWC4KWA" },
              { "name": "AWS_S3_SECRET_ACCESS_KEY","value":"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" },
              { "name": "S3_REGION","value":"ap-south-1" },
              { "name": "S3_DEVICE_LICENSES_BUCKET","value":"coco-developers" },
              { "name": "KUBE_CONFIG_FILE","value":"/etc/kubeconfig" },
              { "name": "TRUST_CENTER_SERVICE_URL","value":"http://trustcenter-server.cocostore.dev.svc.cluster.local:8080" }],
      "ports": [{
        "containerPort": 8080
      }],
      "imagePullPolicy": "Always",
      "volumeMounts": [{
        "mountPath": "/etc/kubeconfig",
        "name": "kubeconfig-file"
      }]
    }],
    "volumes": [{
      "name": "kubeconfig-file",
      "hostPath": {
        "path": "/home/ubuntu/groveapps/config",
        "type": "FileOrCreate"
      }
    }],
    "imagePullSecrets": [{
      "name": "dockercred"
    }],
    "nodeSelector": {
      "kubernetes.io/hostname": "kslave-dev"
    },
    "hostname": "groveproxy",
    "hostNetwork": false,
    "subdomain": "cocostore",
    "dnsPolicy": "ClusterFirst",
    "restartPolicy": "Always"
  }
}
